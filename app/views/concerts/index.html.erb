<p id="notice"><%= notice %></p>

<h1>コンサート</h1>

<table class= "table table-hover">
  <thead class="thead-light">
    <tr>
      <th>情報</th>
      <th>公演日</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concerts.each do |concert| %>
      <tr>
        <td><%= concert.infomation %></td>
        <td><%= concert.date %></td>
        <td><%= link_to '詳細/申込', concert , class:"btn btn-outline-primary"%></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

